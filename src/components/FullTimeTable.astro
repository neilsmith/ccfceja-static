---
const { lrcode, divisionseason } = Astro.props;

const divId = "lrep" + lrcode;
const externalLink = "https://fulltime.thefa.com/index.html?divisionseason=" + divisionseason;
---

<div id={ divId } class="fulltime-table" style="width: 100%;">
  Data loading....
  <a href={ externalLink }>Load League</a>
  <br/><br/>
  <a href="http://www.thefa.com/FULL-TIME">FULL-TIME Home</a>
</div>

<script language="javascript" type="text/javascript" data-lrcode={lrcode}>
  var lrcode = document.currentScript.dataset.lrcode;
</script>
<script language="Javascript" type="text/javascript" src="https://fulltime.thefa.com/client/api/cs1.js"></script>

<style>
  div.fulltime-table { min-height: 400px; background-color: white }
</style>
